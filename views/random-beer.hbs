<section class='random-beers'>
  <ul>
    {{#each random}}
      <img src='{{this.image_url}}' />
      <li> {{this.name}} </li>
      <li> {{this.description}} </li>
      <li> {{this.tagline}} </li>
      <li> {{this.food_pairing}} </li>
      <li> {{this.brewers_tips}} </li>

    {{/each}}
  </ul>
</section>